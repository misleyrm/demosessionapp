    border: 0.5px solid #b8b8b8;              /* Invision : #525D6A;*/
    box-sizing: border-box;
    height: 2px;
    min-width: 100%;
    content: "";
    display: block;
    margin-right: -30px;
}


ul.dropdown-list li> a:hover{
  color: #223042;
}

.dropdown-list.show {
