<div class="section" id="ms-user-new">
  <div class="row">
    <div class="col s12">
      <div class="card">
        <div class="card-title center-align valign-wrapper" >
          <%= image_tag "sessions_logo.png", :size => "192x63", :alt => "Sessiontodos", :title => "Sessiontodos"%>
        </div>
        <div class="white row card-content">
            <div class="col s12 l10 offset-l1">
              <div class="form">
                <%= render 'form' %>
             </div>
            </div>
            <div class="col s12 center">
              <% if !current_user.nil? %>
                <p>Signed in as <%= current_user.first_name.capitalize %> | <%= link_to "Logout", logout_path %></p>
              <% else %>
                <p>Already have an account? <%= link_to "Login", login_path(), class: "text-green"  %></p>
              <% end %>
            </div>
        </div>
      </div>
</div>
</div>
</div>
