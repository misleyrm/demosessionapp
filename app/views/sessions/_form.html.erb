<%= form_for(@session) do |f| %>
<% if @session.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@session.errors.count, "error") %> prohibited this session from being saved:</h2>

  <ul>
    <% @session.errors.full_messages.each do |message| %>
    <li><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>
<div class="row">
  
  <div class="input-field col s12">
    <ul>
      <% @users.each do |user| %>
      <li>
        <div class="field">
          <%= check_box_tag "user_ids[#{user.id}]", "#{user.id}", false, :name => "user_ids[]" %>
          <%= label_tag "user_ids_#{user.id}", user.first_name + " "+ user.last_name %>
        </div>
      </li>
      <% end %>
    </ul>
  </div>
</div>

<div class="row">
  <div class="col s12">
    <a class="actions waves-effect waves-light btn card_submit_button">
      <%= f.submit %>
    </a>
  </div>
</div>

<% end %>
