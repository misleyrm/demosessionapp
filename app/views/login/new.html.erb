<div class="section" id="ms-login">
<div class="row">
  <div class="col s12">
    <div class="card">
      <div class="card-title center-align valign-wrapper" >
        <%= image_tag "sessions_logo.png", :size => "192x63", :alt => "Sessiontodos", :title => "Sessiontodos"%>
      </div>
      <div class="white card-content">
      <div class="row">
        <div class="col s12 l10 offset-l1">
            <h5 class="center-align"></h5>
            <!-- //Welcome back! -->
            <div class="form">
              <%= render 'form' %>
           </div>
        </div>
        <div class="padding-small col s12"></div>
         <div class="center">
           <div class="col s6 m6 l6 right-align border-right">
             <%= link_to "Forgot password?", password_resets_path %>
             <%#= link_to 'Create User', new_user_path %>
           </div>
           <div class="col s6 m6 l6 left-align">
             <%= link_to 'Create Account', new_user_path %>
           </div>
         </div>
       </div>
      </div>
    </div>
  </div>
  </div>
</div>
